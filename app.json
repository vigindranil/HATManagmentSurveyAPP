{
  "expo": {
    "name": "HMSurvey",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "jsEngine": "hermes",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.biplabsvyoma.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow bolt-expo-nativewind to use your location.",
          "locationWhenInUsePermission": "This app requires your location to function correctly."
        }
      ]
     
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.biplabsvyoma.boltexponativewind"
    },
    "extra": {
      "router": {},
      "API_URL_AUTH": "https://wbassetmgmtservice.link/HMSRestAPI/api/auth/authentication",
      "BASE_URL": "https://wbassetmgmtservice.link/HMSRestAPI/api/user",
      "eas": {
        "projectId": "b6a9a2a9-a35b-4f95-b5b5-f65600cdae59"
      }
    }
  }
}
